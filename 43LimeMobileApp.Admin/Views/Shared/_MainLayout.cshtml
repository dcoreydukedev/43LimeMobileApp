<!DOCTYPE html>

<html lang="en-us" dir="ltr">

<head>
    <meta charset="utf-8" />
    <meta content="width=device-width, initial-scale=1.0, maximum-scale=2.0, shrink-to-fit=no" name="viewport" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />

    <title>43 Lime @ViewBag.Title</title>

    <!--     Css     -->
    <link href="~/Styles/bootstrap/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/jquery-ui/jquery-ui.min.css" rel="stylesheet" />
    <link href="~/Content/mdb/css/mdb.css" rel="stylesheet" />
    <link href="~/Styles/colors.css" rel="stylesheet" />
    <link href="~/Content/toastr/toastr.css" rel="stylesheet" />
    <link href="~/Styles/animate/animate.min.css" rel="stylesheet" />
    <link href="~/Styles/Site.css" rel="stylesheet" />

    @RenderSection("CSS", required: false)

    <!-- JS -->
    <script src="~/Scripts/jquery/jquery-3.4.1.min.js"></script>
    <script src="~/Content/jquery-ui/jquery-ui.min.js"></script>
    <script src="~/Scripts/jquery/jquery-migrate/jquery-migrate.min.js"></script>
    <script src="~/Scripts/jquery/jquery-unobtrusive-ajax/jquery.unobtrusive-ajax.js"></script>
    <script src="~/Scripts/jquery/jquery-validate/jquery.validate.js"></script>
    <script src="~/Scripts/jquery/jquery-validate/jquery.validate.unobtrusive.js"></script>
    <script src="~/Scripts/bootstrap/bootstrap.bundle.min.js"></script>
    <script src="~/Content/toastr/toastr.js"></script>
    <script src="~/Scripts/plugins/jquery.lettering.js"></script>
    <script src="~/Scripts/plugins/jquery.textillate.js"></script>
    <script src="~/Scripts/cookies.js"></script>
    <script src="~/Scripts/Site.js"></script>

    <!-- Font Awesome -->
    <script src="https://kit.fontawesome.com/6f8ddbde0a.js" type="text/javascript"></script>

    @RenderSection("JS", required: false)
</head>

<body id="main_body" style="z-index:-100;">

    @RenderBody()

    @RenderSection("scripts", required: false)
</body>
</html>
